// })

$("#SubscribeForm").validate(
        {
        rules: {
        email: {
        required: true,
        email: true
        },
        zipcode:{
        required: true,
        },
        },
        messages: {
        email: {
        email: "Your email address must be in the format of name@domain.com"
        }
        },
        submitHandler: function (form) {
        $.ajax({
        method:"POST",
        url:"/subscription",
        data:{email:$('#subsEmail').val(),zipcode:$('#subsZipcode').val()},
        success:function(res){
        console.log(res);
        if(res.Success==true){
        $('.subSuccess').html(res.Message);
        $('.success').fadeOut(10000);
        }else{
        $('.subError').html(res.Message);
        $('.error').fadeOut(10000);
        }
        }
        });           
    }
    }
    );


</script>